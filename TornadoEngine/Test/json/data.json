{
    "targetForParsing": {
        "directories": [
            "./DataSources"
        ],
        "recursive": true
    },
    "filter": {
        "attribute": "",
        "extensions": [
            ".h",
            ".hpp",
            ".hh",
            ".hxx",
            ".h++"
        ]
    },
    "targetForCodeGeneration": {
        "directory": ".",
        "includeListFileName": "IncludeList",
        "header": "test",
        "typeCustomizerMap": {
            "int8_t": "char",
            "uint8_t": "unsigned char",
            "int16_t": "short",
            "uint16_t": "unsigned short",
            "int32_t": "int",
            "uint32_t": "unsigned int",
            "VectorStatic": "vector"
        },
        "implementation": {
            "jsonSerializer": {
                "exportDeclaration": "",
                "nameSpaceName": "nsJson",
                "fileName": "JsonSerializer",
                "className": "TJsonSerializer"
            },
            "binaryMarshaller": null
        }
    }
}
