{
    "targetForParsing": {
        "directories": [
            "."
        ],
        "recursive": true
    },
    "filter": {
        "attribute": "REFLECTION_ATTRIBUTE",
        "extensions": [
            ".h",
            ".hpp",
            ".hh",
            ".hxx",
            ".h++"
        ]
    },
    "targetForCodeGeneration": {
        "directory": ".",
        "includeListFileName": "IncludeList",
        "header": "\tReflectionCodeGenerator",
        "typeCustomizerMap": {
            "int8_t": "char",
            "uint8_t": "unsigned char",
            "int16_t": "short",
            "uint16_t": "unsigned short",
            "int32_t": "int",
            "uint32_t": "unsigned int",
            "vector_static_t": "vector"
        },
        "implementation": {
            "jsonSerializer": {
                "exportDeclaration": "",
                "nameSpaceName": "",
                "fileName": "GameEngineJsonSerializer",
                "className": "TGameEngineJsonSerializer"
            }
        }
    }
}
