{
    "targetForParsing": {
        "directories": [
            "./Sources"
        ],
        "recursive": true
    },
    "filter": {
        "attribute": "REFLECTION_ATTRIBUTE",
        "extensions": [
            ".h",
            ".hpp",
            ".hh",
            ".hxx",
            ".h++"
        ]
    },
    "targetForCodeGeneration": {
        "directory": "./Sources",
        "includeListFileName": "IncludeList",
        "header": "\tReflectionCodeGenerator",
        "typeCustomizerMap": {
            "int8_t": "char",
            "uint8_t": "unsigned char",
            "int16_t": "short",
            "uint16_t": "unsigned short",
            "int32_t": "int",
            "uint32_t": "unsigned int",
            "vector_static_t": "vector"
        },
        "implementation": {
            "jsonSerializer": {
                "exportDeclaration": "",
                "nameSpaceName": "nsJson",
                "fileName": "JsonSerializer",
                "className": "TJsonSerializer"
            },
            "binaryMarshaller": {
                "exportDeclaration": "",
                "nameSpaceName": "nsBinary",
                "fileName": "BinaryMarshaller",
                "className": "TBinaryMarshaller",
                "beginID": 666,
                "unpackHandler": {
                    "exportDeclaration": "",
                    "nameSpaceName": "",
                    "fileName": "UnpackHandler",
                    "className": "TUnpackHandler",
                    "method": "Handle"
                },
            "sql": null
            }
        }
    }
}
