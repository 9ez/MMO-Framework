{
    "targetForParsing": {
        "directories": [
            "."
        ],
        "recursive": true
    },
    "filter": {
        "attribute": "REFLECTION_ATTRIBUTE",
        "extensions": [
            ".h",
            ".hpp",
            ".hh",
            ".hxx",
            ".h++"
        ],
        "inheritances": [],
        "memberIgnore": "IGNORE_ATTRIBUTE"
    },
    "targetForCodeGeneration": {
        "directory": "./Generated files",
        "includeListFileName": "ReflectionIncludeList",
        "header": "\tReflectionCodeGenerator",
        "typeCustomizerMap": {},
        "appendTypeCustomizerMap": true,
        "implementations": {
            "JsonSerializerGenerator": {
                "exportDeclaration": "DllExport",
                "nameSpaceName": "nsTornadoEngine",
                "fileName": "TornadoEngineJsonSerializer",
                "className": "TTornadoEngineJsonSerializer",
                "keyValueMap": {}
            }
        }
    }
}